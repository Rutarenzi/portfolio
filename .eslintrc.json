{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
	"overrides": [
		{
			"files": ["src/**/*.spec.js", "src/**/*.spec.jsx"],
			"env": {
				"jest": true
			}
		}
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {}
}
